<?xml version="1.0" encoding="utf-8"?>

<com.yongchul.suitdagenealogy.modules.customview.CustomView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CC000000"
    android:orientation="vertical"
    >

    <com.yongchul.suitdagenealogy.modules.customview.CustomView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/main_color_white"
        android:gravity="center"
        android:layout_gravity="center"
        app:h="200"
        app:w="300">

        <com.yongchul.suitdagenealogy.modules.customview.CustomTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:h="40"
            android:text="당신의 결과"
            android:textColor="@color/main_color_black"
            android:gravity="center"
            />

        <com.yongchul.suitdagenealogy.modules.customview.CustomView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <com.yongchul.suitdagenealogy.modules.customview.CustomImgView
                android:id="@+id/subV_img1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"
                android:layout_weight="1"/>

            <com.yongchul.suitdagenealogy.modules.customview.CustomImgView
                android:id="@+id/subV_img2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"
                android:layout_weight="1"/>

        </com.yongchul.suitdagenealogy.modules.customview.CustomView>


    </com.yongchul.suitdagenealogy.modules.customview.CustomView>


</com.yongchul.suitdagenealogy.modules.customview.CustomView>



